<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.employbee" >

    <!-- Stuff to allow for reading and writing excel file -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EmployBee" >
        <activity

            android:name=".create_room"
            android:exported="false" />
        <activity
            android:name=".login_attempt"
            android:exported="false" />

            android:name=".AddTasks"
            android:exported="false" />
        <activity
            android:name=".sampleShelf"
            android:exported="false"
            android:screenOrientation="landscape" />
        <activity
            android:name=".sampleRoom"
            android:exported="false"
            android:screenOrientation="landscape" />

        <activity
            android:name=".general_map"
            android:exported="false"
            android:screenOrientation="landscape" />
        <activity
            android:name=".protocol_page"
            android:exported="false"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".Tasks1Activity"
            android:exported="false"
            android:label="@string/title_activity_tasks1"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>